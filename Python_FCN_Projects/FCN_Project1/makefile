#!/bin/bash
chmod 777 client
echo -e "Non ssl (./client -p 27993 cs5700sp17.ccs.neu.edu 001256309)\n"
./client -p 27993 cs5700sp17.ccs.neu.edu 001256309
echo -e "Default :Non ssl (./client cs5700sp17.ccs.neu.edu 001256309)\n"
./client cs5700sp17.ccs.neu.edu 001256309
echo -e "SSL (./client -p 27994 -s cs5700sp17.ccs.neu.edu 001256309)\n"
./client -p 27994 -s cs5700sp17.ccs.neu.edu 001256309
echo -e "Default: SSL (./client -s cs5700sp17.ccs.neu.edu 001256309)\n"
./client -s cs5700sp17.ccs.neu.edu 001256309
